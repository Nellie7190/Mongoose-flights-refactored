<%- include('../partials/header') %>

 <form id='add-ticket-form' action='/flights/<%= flightId %>/tickets' method='POST'>
    <label>Seat: </label>
    <input name='seat' require pattern='[A-F][1-9]\d?'>
    <label>Price: $</label>
    <input type='number' name='price'>
    <input id='submit-ticket-button' type='submit' value='Submit Ticket'>
</form> 
